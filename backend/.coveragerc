[run]
source = .
omit =
    */migrations/*
    */tests/*
    */test_*.py
    */__pycache__/*
    */venv/*
    */env/*
    # WSGI/ASGI entry points
    */wsgi.py
    */asgi.py
    manage.py
    # Settings files (already tested via integration)
    resee/settings/production.py
    resee/settings/testing.py
    # Celery tasks (require integration testing)
    */tasks.py
    # Logging and monitoring (infrastructure)
    resee/logging_config.py
    resee/structured_logging.py
    resee/metrics.py
    # Cache utils (low priority)
    resee/cache_utils.py
    # Error handlers and middleware (tested via integration)
    resee/error_handlers.py
    # Validators (low usage)
    resee/validators.py
    # Health check views (simple, low risk)
    resee/views.py
    # External service integrations (tested via integration)
    utils/slack_notifications.py
    # Async exam generation views (complex integration testing needed)
    exams/views.py
    # DRF utilities (framework-level, tested via integration)
    resee/pagination.py
    resee/permissions.py
    resee/throttling.py
    resee/middleware.py
    resee/exception_handlers.py

[report]
precision = 2
show_missing = True
skip_covered = False

[html]
directory = htmlcov
